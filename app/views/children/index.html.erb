<% provide(:title, 'All Children') %>
<h1>All Children</h1>

<%= will_paginate %>

<ul class="objects">
  <% @children.each do |child| %>
    <li>
		<a href="<%= edit_child_path(child.id) %>">
		<h4><%= child.fname %>
			<%= child.mname %>
			<%= child.lname %></h4>
		</a>
    </li>
  <% end %>
</ul>


